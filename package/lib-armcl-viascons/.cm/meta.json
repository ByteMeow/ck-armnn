{
  "customize": {
    "extra_dir": "", 
    "git_src_dir": "src", 
    "install_env": {
      "PACKAGE_BUILD_TYPE": "skip", 
      "PACKAGE_GIT": "YES", 
      "PACKAGE_SKIP_CLEAN_OBJ": "YES", 
      "PACKAGE_SKIP_CLEAN_SRC_DIR": "YES", 
      "PACKAGE_SKIP_LINUX_MAKE": "YES", 
      "PACKAGE_SUB_DIR": "src", 
      "PACKAGE_URL": "https://github.com/ARM-software/ComputeLibrary",
      "PACKAGE_VERSION": "release",
      "USE_NEON": "NO",
      "USE_OPENCL": "NO"
    }
  }, 
  "deps": {
    "scons": {
      "local": "yes",
      "name": "SCons tool",
      "sort": 10,
      "tags": "tool,scons"
    }
  },
  "end_full_path_universal": "src$#sep#$build$#sep#$libarm_compute$#file_ext_dll#$", 
  "need_cpu_info": "yes", 
  "only_for_host_os_tags": [
    "linux"
  ], 
  "only_for_target_os_tags": [
    "linux"
  ], 
  "process_script": "install", 
  "soft_uoa": "lib.armcl", 
  "suggested_path": "lib-armcl", 
  "tags": [
    "lib", 
    "armcl", 
    "viascons"
  ], 
  "variations": {
    "rel.18.08": {
        "extra_env": {
            "PACKAGE_GIT_CHECKOUT": "v18.08",
            "PACKAGE_VERSION": "18.08"
        }
    },
    "release": {
    },
    "dev": {
        "extra_env": {
            "PACKAGE_URL": "https://review.mlplatform.org/ml/ComputeLibrary",
            "PACKAGE_VERSION": "dev"
        }
    },
    "neon": {
        "extra_env": {
            "USE_NEON": "YES"
        }
    },
    "opencl": {
        "extra_env": {
            "USE_OPENCL": "YES"
        }
    }
  },
  "use_scripts_from_another_entry": {
    "data_uoa": "download-and-install-package", 
    "module_uoa": "script"
  }
}
