{
  "customize": {
    "extra_dir": "", 
    "git_src_dir": "src", 
    "install_env": {
      "PACKAGE_BUILD_TYPE": "skip", 
      "PACKAGE_GIT": "YES", 
      "PACKAGE_SKIP_CLEAN_OBJ": "YES", 
      "PACKAGE_SKIP_CLEAN_SRC_DIR": "YES", 
      "PACKAGE_SUB_DIR": "src", 
      "PACKAGE_URL": "https://github.com/arm-software/armnn",
      "USE_NEON": "NO",
      "USE_OPENCL": "NO",
      "USE_TF": "NO",
      "USE_TFLITE": "NO",
      "USE_ONNX": "NO"
    }, 
    "use_git_revision": "yes", 
    "version": "default"
  }, 
  "deps": {
    "lib-boost": {
      "local": "yes", 
      "name": "Boost library", 
      "skip_default": "yes", 
      "sort": 10, 
      "tags": "lib,boost,for-armnn", 
      "version_from": [
        1, 
        64, 
        0
      ]
    }, 
    "lib-protobuf": {
      "local": "yes", 
      "name": "ProtoBuf library", 
      "sort": 20, 
      "tags": "lib,protobuf-host", 
      "version_from": [
        3, 
        5, 
        0
      ], 
      "version_to": [
        3, 
        5, 
        1
      ]
    }, 
    "tensorflow-source": {
      "enable_if_env": [
        {
            "USE_TF": ["on", "ON", "yes", "YES", "1"]
        },
        {
            "USE_TFLITE": ["on", "ON", "yes", "YES", "1"]
        }
      ],
      "local": "yes", 
      "name": "Tensorflow source", 
      "sort": 30, 
      "tags": "tensorflow,source"
    },
    "lib-armcl-neon": {
      "enable_if_env": {
            "USE_NEON": ["on", "ON", "yes", "YES", "1"],
            "USE_OPENCL": ["off", "OFF", "no", "NO", "0"]
      },
      "local": "yes",
      "name": "Arm Compute Library with Neon support",
      "sort": 40,
      "tags": "lib,armcl,vneon"
    },
    "lib-armcl-opencl": {
      "enable_if_env": {
        "USE_NEON": ["off", "OFF", "no", "NO", "0"],
        "USE_OPENCL": ["on", "ON", "yes", "YES", "1"]
      },
      "local": "yes",
      "name": "Arm Compute Library with OpenCL support",
      "sort": 40,
      "tags": "lib,armcl,vopencl"
    },
    "lib-armcl-neon-opencl": {
      "enable_if_env": {
        "USE_NEON": ["on", "ON", "yes", "YES", "1"],
        "USE_OPENCL": ["on", "ON", "yes", "YES", "1"]
      },
      "local": "yes",
      "name": "Arm Compute Library with Neon and OpenCL support",
      "sort": 40,
      "tags": "lib,armcl,vneon,vopencl"
    },
    "lib-flatbuffers": {
      "enable_if_env": {
        "USE_TFLITE": ["on", "ON", "yes", "YES", "1"]
      },
      "local": "yes",
      "name": "Google FlatBuffers library",
      "sort": 50,
      "tags": "lib,flatbuffers"
    },
    "compiler": {
      "local": "yes",
      "name": "C++ compiler",
      "sort": 60,
      "tags": "compiler,lang-cpp"
    }
  }, 
  "end_full_path_universal": "install$#sep#$lib$#sep#$libarmnn$#file_ext_dll#$", 
  "need_cpu_info": "yes", 
  "only_for_host_os_tags": [
    "linux"
  ], 
  "only_for_target_os_tags": [
    "linux"
  ], 
  "process_script": "install", 
  "soft_uoa": "lib.armnn", 
  "suggested_path": "lib-armnn", 
  "tags": [
    "lib", 
    "armnn"
  ], 
  "use_preprocess_scripts_from_another_entry": {
    "data_uoa": "process-compiler-for-cmake",
    "module_uoa": "script"
  },
  "use_scripts_from_another_entry": {
    "data_uoa": "download-and-install-package", 
    "module_uoa": "script"
  }
}
